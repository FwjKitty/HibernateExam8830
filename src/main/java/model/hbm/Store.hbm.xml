<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="model">
   <class name="Store" table="store">
      <meta attribute="class-description">
         This class contains the store detail. 
      </meta>
      <id name="store_id" type="int" column="store_id">
         <generator class="increment"/>
      </id>
      <property name="manager_staff_id" column="manager_staff_id" type="int"/>
      <property name="address_id" column="address_id" type="timestamp"/>
   	  <property name="last_update" column="last_update" type="timestamp"/>
   </class>
</hibernate-mapping>